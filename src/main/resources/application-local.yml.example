spring:
  datasource:
    url: jdbc:mysql://<DB_HOST>:3306/amazon?characterEncoding=UTF-8&serverTimezone=UTC
    username: <DB_USERNAME>
    password: <DB_PASSWORD>
  jpa:
    show-sql: true
    properties:
      hibernate.hibernate.format_sql: true
springdoc:
  api-docs:
    enabled: true
  swagger-ui:
    enabled: true
